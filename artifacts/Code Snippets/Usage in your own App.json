	"name": "Usage in your own App",
	"description": null,
	"group": "Neptune FORMS",
	"content": [
		"==OBJECT STRING==",
		"// Start new FORM\r",
		"FORMS.build(parentObject, formId);\r",
		"\r",
		"// Get Data from FORM\r",
		"let formData = FORMS.getData();\r",
		"\r",
		"// Get Data from FORM - And set to completed if all required fields are OK\r",
		"let formData = FORMS.getData(true);\r",
		"\r",
		"/* The FORM Data structure will be. \r",
		"    formData = {\r",
		"        data: formData,\r",
		"        config: formConfig,\r",
		"        completed: true / false,\r",
		"        valid: true / false,\r",
		"    }\r",
		"*/\r",
		"\r",
		"// Start FORM with Data\r",
		"FORMS.build(parentObject, {\r",
		"    id: formId,\r",
		"    data: formData,\r",
		"    config: formConfig,\r",
		"});"
	],
